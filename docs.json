  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Integrate SDK",
        "href": "https://github.com/mayan-finance/swap-sdk"
      },
      {
