      "copy",
      "view",
      "chatgpt",
      "claude"
    ]
  },
  "background": {
    "decoration": "grid",
    "color": {
      "dark": "#1A1A1F"
    }
  },
  "fonts": {
    "heading": {
      "family": "Space Grotesk"
    },
    "body": {
      "family": "Titillium Web"
    }
  },
  "favicon": "/favicon.jpg",
  "navigation": {
    "groups": [
      {
        "group": " ",
        "pages": [
          "index"
        ]
      },
      {
