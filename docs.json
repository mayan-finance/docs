    "global": {
      "anchors": [
        {
          "anchor": "Community",
          "href": "https://discord.com/invite/MayanFinance",
          "icon": "discord"
        },
        {
          "anchor": "Blog",
          "href": "https://blog.mayan.finance/",
          "icon": "newspaper"
        },
        {
          "anchor": "X",
          "href": "https://twitter.com/mayanfinance",
          "icon": "x-twitter"
        }
      ]
    }
